<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginScreenController">
   <children>
      <Button fx:id="loginButton" layoutX="185.0" layoutY="268.0" mnemonicParsing="false" onAction="#clickLogin" prefHeight="39.0" prefWidth="90.0" text="Login">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="exitButton" layoutX="481.0" layoutY="25.0" mnemonicParsing="false" onAction="#clickExit" prefHeight="39.0" prefWidth="90.0" text="Exit">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Label fx:id="zone" layoutX="25.0" layoutY="25.0" prefHeight="27.0" prefWidth="51.0" text="Zone: ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="185.0" layoutY="151.0" prefHeight="38.0" prefWidth="342.0" promptText="Please enter your username">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordTextField" layoutX="185.0" layoutY="211.0" prefHeight="38.0" prefWidth="342.0" promptText="Please enter your password">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label fx:id="usernameLabel" layoutX="84.0" layoutY="157.0" text="Username:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="90.0" layoutY="217.0" text="Password:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="zoneLabelBlank" layoutX="76.0" layoutY="25.0" prefHeight="27.0" prefWidth="259.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
